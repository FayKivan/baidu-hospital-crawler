            <div title="<#=detailTblBody[i][j].name#>" <#if (j == 2) {#>class="notice-time"<#}#>><#=detailTblBody[i][j].name#></div>  
            <#}#>
        </td>
        <#}#>
    </tr>
    <#}#>
</script>
